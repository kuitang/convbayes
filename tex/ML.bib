	Keywords = {audio coding;content-based retrieval;harmonic analysis;learning (artificial intelligence);music;support vector machines;HMM;LabROSA supervised chord recognition system;chord transcription;content-based retrieval;harmonic analysis;hidden Markov model;indexing;machine learning methods;music audio;nonlinear kernels;structural information;structured prediction models;Content based retrieval;Decoding;Harmonic analysis;Hidden Markov models;Indexing;Kernel;Labeling;Machine learning;Music information retrieval;Predictive models;acoustic features;chord transcription;classification and prediction;structured and relational data},
