	Issn = {1053-5888},
	Journal = {Signal Processing Magazine, IEEE},
	Keywords = {Bayes methods;optimisation;signal reconstruction;compressive sensing;convex reconstruction;low-level signal properties;optimization criterion;point estimation;posterior distribution;probabilistic Bayesian context;sparse signal reconstruction;variational Bayesian inference techniques;Approximation algorithms;Approximation methods;Bayesian methods;Estimation;Image reconstruction;Inference algorithms;Signal processing algorithms},
