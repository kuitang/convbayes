	Journal = {Journal of Mathematical Psychology},
	Number = {2},
	Pages = {107 - 134},
	Title = {Binary choice, subset choice, random utility, and ranking: A unified perspective using the permutahedron},
	Url = {http://www.sciencedirect.com/science/article/pii/S0022249603001020},
	Volume = {48},
	Year = {2004},
